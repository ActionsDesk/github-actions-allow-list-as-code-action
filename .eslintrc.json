{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github"],
  "extends": ["plugin:github/recommended"],
  "rules": {
    "camelcase": [
      "error",
      {
        "allow": [""]
      }
    ],
    "filenames/match-regex": "off",
    "import/named": "off",
    "import/namespace": "off",
    "import/no-deprecated": "off",
    "no-console": "off"
  }
}
